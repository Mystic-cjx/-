#include<iostream>
using namespace std;
int main() {
	int arr1[3][3] = {
		{100,100,100},
		{30,30,30},
		{40,40,40}
	};
	string name[3] = { "张三","李四","王五" };
	for (int i = 0; i < 3; i++)
	{
		int sum = 0;
		for (int j = 0; j < 3; j++)
		{
			sum += arr1[i][j];


		}
		cout << name[i]<<"的总分是" << sum << endl;
	}
}
